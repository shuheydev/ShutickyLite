<Window x:Class="ShutickyLite.SettingWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ShutickyLite"
        mc:Ignorable="d"
        Title="設定" Height="200" Width="200" ResizeMode="NoResize" Closing="Window_Closing" >
    <Grid>
        <CheckBox Name="CheckBox_ExecuteAtStartup" Content="起動時に実行" HorizontalAlignment="Left" Margin="10,23,0,0" VerticalAlignment="Top" Height="18" Width="153"/>
        <Button Name="Button_ApplySetting" Content="OK" HorizontalAlignment="Right" Margin="0,0,109.667,10" VerticalAlignment="Bottom" Width="75" Click="Button_ApplySetting_Click"/>
        <Button Name="Button_CancelSetting" Content="キャンセル" HorizontalAlignment="Right" Margin="0,0,9.667,10" VerticalAlignment="Bottom" Width="75" Click="Button_CancelSetting_Click"/>

    </Grid>
</Window>
